{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"jsx": "preserve",
		"allowJs": true,
		"checkJs": true,
		"noEmit": true,
		"types": ["vite/client"],
		"isolatedModules": true
	}
}
